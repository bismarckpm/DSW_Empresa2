    <class>ucab.dsw.entidades.Estudio</class>
    <class>ucab.dsw.entidades.EstudioLugar</class>
    <class>ucab.dsw.entidades.Hijo</class>
    <class>ucab.dsw.entidades.HistoricoEstado</class>
    <class>ucab.dsw.entidades.Informacion</class>
    <class>ucab.dsw.entidades.Lugar</class>
    <class>ucab.dsw.entidades.Marca</class>
    <class>ucab.dsw.entidades.MedioComunicacion</class>
    <class>ucab.dsw.entidades.NivelAcademico</class>
    <class>ucab.dsw.entidades.NivelEconomico</class>
    <class>ucab.dsw.entidades.Ocupacion</class>
    <class>ucab.dsw.entidades.PreguntaEncuesta</class>
